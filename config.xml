<?xml version='1.0' encoding='utf-8'?>
<widget android-activityName="DoMore" id="com.hybrid.doapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DoMore</name>
    <description>
         Hybrid mobile application
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-sqlite-storage" spec="^2.3.1" />
    <plugin name="cordova-plugin-dbcopy" spec="^2.0.0" />
    <plugin name="cordova-plugin-password-crypto" spec="^1.0.0" />
    <preference name="appVersion" value="0.1" />
    <preference name="dbName" value="doapp.db" />
    <preference name="dbVersion" value="0.1" />
    <preference name="dbUpgradeMode" value="1" />
    <plugin name="cordova-plugin-customconfigparameters" spec="^3.0.0" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <engine name="android" spec="^7.0.0" />
</widget>
